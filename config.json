{
  "description": "Redis log driver",
  "documentation": "-",
  "entrypoint": ["/usr/bin/docker-redis-log-driver"],
  "network": {
    "type": "host"
  },
  "interface": {
    "types": [
      "docker.logdriver/1.0"
    ],
    "socket": "log.sock"
  },
	"env": [
		{
			"name": "LOG_LEVEL",
			"description": "Set log level to output for plugin logs",
			"value": "info",
			"settable": ["value"]
		}
	]
}
